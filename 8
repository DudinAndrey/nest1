program eight
implicit NONE
integer i,j
integer,parameter::n=4
integer a(n,n), b(n,n), c(n,n)

do i=1,n 
    do j=1,n 
        read(*,'(i3.1$)') a(i,j)
    enddo 

enddo 

do i=1,n 
    do j=1,n 
        read(*,'(i3.1$)') b(i,j)
    enddo

enddo 

do i=1,n 
    do j=1,n 
        if (a(i,j).GT.b(i,j)) then
         write(*,'(i3.1$)') a(i,j)
        ELSE
         write(*,'(i3.1$)') b(i,j)
        endif 
    enddo
write(*,*)
enddo 

end program eight
