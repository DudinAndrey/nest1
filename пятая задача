program five
implicit none
integer x0,xk,dx,y0,yk,dy,y,y2,x,a,b
integer jmax,imax,j,i

read(*,*) x0,xk,dx,y0,yk,dy
read(*,*) a,b 
jmax=(yk-y0)/dy+0.5+1
imax=(xk-x0)/dx+0.5+1

do j=1,jmax
    y=yk-(j-1)*dy
    y2=y*y
    do i=1,imax
        x=x0+(i-1)*dx
        if ( (((x**2)/(a**2)+(y**2)/(b**2)).LE.1) .and. (y-x.GT.0) .and. (y.GT.0)) then
        write(*,"(A2,$)")' *'
        else 
        write (*,"(A2,$)")' .'
        endif
    enddo
   write(*,*) 
enddo

pause
end program five
